{
  "name": "cityvizor-client",
  "version": "4.2.0",
  "description": "CityVizor web client for CityVizor API (Server)",
  "homepage": "https://cityvizor.cz/",
  "repository": "otevrena-data-mfcr/CityVizor",
  "main": "server/server.js",
  "scripts": {
    "build": "cp -R node_modules/bootstrap-sass/assets/fonts node_modules/bootstrap-sass/assets/stylesheets/ && ng build --prod",
    "start-dev": "npm run build && ng serve --disable-host-check --configuration=local --host 0.0.0.0"
